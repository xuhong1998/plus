.calendar{
  background-color: #fff;
  padding-bottom: 40rpx;
}
.calendar {
    background-color:white;  
    padding-top: 10px; 
}
.calendar-header{
  font-size: large;
  color: #59518d;
}
.calendar-board{
  color: #c7cbe2;
   font-weight: bold;
}
.wx-time{
  background-color: #fff;
  padding-bottom:40rpx;
}
.wx-item-head{
  border-bottom:2rpx solid #e2e2e2;
  padding: 0rpx 35rpx;
}
.wx-box{
  display: flex;
  min-height:150rpx;
}
.wx-box-name text:nth-child(1){
  display: block;
  font-size:35rpx;
}
.wx-box-name text:nth-child(2){
  width:500rpx;
  display:block;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
  font-size:25rpx;
  color: #8799a3;
}
.wx-box-day{
  width:90rpx;
  padding-top:35rpx;
  text-align:right;
  padding-right:20rpx;
  color: #8799a3;
}
.wx-box-name{
  width:530rpx;
  min-height:120rpx;
  align-items:center;
  border-bottom:2rpx dashed #8799a3;
  padding-left: 20rpx;
  padding-top:30rpx;
}
.wx-box-distanc{
  width: 150rpx;
}
.wx-box-llish{
  width: 0rpx;
}
.wx-box-llish view:nth-child(1){
  height: 40rpx;
  width: 0rpx;
  border: 1rpx #e54d42 solid;
}
.wx-box-llish view:nth-child(2){
  height:20rpx;
  width:20rpx;
  border-radius:50%;
  background:#e54d42;
  position:relative;
  left:-5px;
}
.wx-box-llish view:nth-child(3){
  height: 90rpx;
  border: 1rpx #e54d42 solid;
  width: 0rpx;
}
.wx-box-distance{
  margin:auto 10rpx;
  font-size:32rpx;
  color:#e54d42;
  font-weight:600;
}
.bottom{
  width: 750rpx;
  height: 20rpx
}
.solid{
  height: 20rpx;
  width: 0rpx;
  position: relative;
  left: 30rpx;
  top: 0rpx;
  border: 1rpx #2e2e2e solid;
}
.wx-time-body .wx-box:nth-last-of-type(1) .wx-box-name{
  border: none;
}
.wx-item-left{
  padding-left:20rpx;
  color:#666;
  border-left:10rpx solid #f37b1d;
  border-radius:8rpx;
  flex: 1;
}
.wx-time-head view:nth-child(2){
   margin-right: 20rpx;
   font-size: 26rpx;
}